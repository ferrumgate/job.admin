    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "linux": {},
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/build",
            "program": "${workspaceFolder}/build/src/main.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "REDIS_HOST": "localhost:6379",
            },
            "outputCapture": "console",
            "preLaunchTask": "build"
        }
    ]
}